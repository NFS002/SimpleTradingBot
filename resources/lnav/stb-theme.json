{
  "$schema": "https://lnav.org/schemas/config-v1.schema.json",
  "ui": {
    "theme-defs": {
      "stb": {
        "vars": {
          "black": "#2d2a2e",
          "red": "#ff6868",
          "green": "#007f00",
          "lime": "#00ff00",
          "yellow": "#cdcd00",
          "blue": "#5394ec",
          "magenta": "#ff70ff",
          "cyan": "#33cccc",
          "white": "#808080",
          "purple": "#af87ff",
          "aquamarine": "#87ffd7"
        },
        "styles": {
          "identifier": {
            "color": "Gold1"
          },
          "text": {
            "color": "$cyan",
            "background-color": "#011627"
          },
          "alt-text": {
            "color": "$magenta",
            "bold": true
          },
          "ok": {
            "color": "$green",
            "bold": true
          },
          "error": {
            "color": "$red",
            "bold": true
          },
          "warning": {
            "color": "$yellow",
            "bold": true
          },
          "hidden": {
            "color": "$yellow",
            "bold": true
          },
          "adjusted-time": {
            "color": "$magenta"
          },
          "skewed-time": {
            "color": "$yellow"
          },
          "offset-time": {
            "color": "$cyan"
          }
        },
        "syntax-styles": {
          "keyword": {
            "color": "$green"
          },
          "string": {
            "color": "$green",
            "bold": true
          },
          "number": {
            "color": "$purple",
            "bold": true
          }
        },
        "log-level-styles": {
          "warning": {
            "color": "$yellow"
          },
          "error": {
            "color": "$red"
          },
          "info" : {
            "color": "$blue"
          }
        }
      }
    }
  }
}
